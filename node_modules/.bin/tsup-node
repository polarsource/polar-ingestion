#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/tsup@8.3.5_@microsoft+api-extractor@7.43.0_@types+node@22.13.1__jiti@2.4.2_postcss@8.4.49_typescript@5.7.3_yaml@2.7.0/node_modules/tsup/dist/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/tsup@8.3.5_@microsoft+api-extractor@7.43.0_@types+node@22.13.1__jiti@2.4.2_postcss@8.4.49_typescript@5.7.3_yaml@2.7.0/node_modules/tsup/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/tsup@8.3.5_@microsoft+api-extractor@7.43.0_@types+node@22.13.1__jiti@2.4.2_postcss@8.4.49_typescript@5.7.3_yaml@2.7.0/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/tsup@8.3.5_@microsoft+api-extractor@7.43.0_@types+node@22.13.1__jiti@2.4.2_postcss@8.4.49_typescript@5.7.3_yaml@2.7.0/node_modules/tsup/dist/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/tsup@8.3.5_@microsoft+api-extractor@7.43.0_@types+node@22.13.1__jiti@2.4.2_postcss@8.4.49_typescript@5.7.3_yaml@2.7.0/node_modules/tsup/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/tsup@8.3.5_@microsoft+api-extractor@7.43.0_@types+node@22.13.1__jiti@2.4.2_postcss@8.4.49_typescript@5.7.3_yaml@2.7.0/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
