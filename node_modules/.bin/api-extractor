#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules/@microsoft/api-extractor/bin/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules/@microsoft/api-extractor/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules/@microsoft/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules/@microsoft/api-extractor/bin/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules/@microsoft/api-extractor/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules/@microsoft/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules:/Users/ewidlund/dev/polar-adapters/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules/@microsoft/api-extractor/bin/api-extractor" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.17.12/node_modules/@microsoft/api-extractor/bin/api-extractor" "$@"
fi
